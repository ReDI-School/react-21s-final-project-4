<script>
  jQuery(document).ready(function($){
    $("#show-post-comments").click(function(e) {
      e.preventDefault();
      $("#comments-loader").html("Loading comments...");
      var data = {
        action: 'get_lazy_comments',
        post_id: $("#comments-loader").data('postid')
      };
      $.post(sage_ajax.ajaxurl, data, function(response) {
        $("#comments-loader").html(response);
        $("#hide-post-comments").show();
      });
      $("#show-post-comments").hide();
    });
    $("#hide-post-comments").click(function(e) {
      e.preventDefault();
      $("#comments-loader").html("");
      $("#hide-post-comments").hide();
      $("#show-post-comments").show();
    });
  });
</script>
